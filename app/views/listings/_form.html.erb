<%= form_for @listing do |f|  %>
  <%= f.label :name %>
  <%= f.text_field :name, :required => true %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.label :image %>
  <%= f.url_field :image %>

  <%= f.submit %>
<% end %>
